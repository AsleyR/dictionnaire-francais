generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model verbes {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  qqch      String?
  qqn       String?
  remarques String?
  verbe     String
  vi        String?
}
